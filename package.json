  "description": "Starter-Next-AntD-Express",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w server ./src/server/server.js --exec babel-node -e js",
    "build": "NODE_ENV=production next build; babel src -d dist --copy-files",
    "start": "NODE_ENV=production node ./dist/server/server.js",
    "server": "NODE_ENV=production node ./dist/server/server.js",
    "test": "NODE_ENV=test jest",
    "test:unit": "NODE_ENV=test jest test/**/*.unit.spec.js",
    "test:integration": "NODE_ENV=test jest test/**/*.int.spec.js",
    "test:watch": "jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:coverage:unit": "jest --coverage test/**/*.unit.spec.js",
    "test:coverage:integration": "jest --coverage test/**/*.int.spec.js"
  },
