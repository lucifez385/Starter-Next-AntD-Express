  "author": "Joe Watchapon",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@zeit/next-less": "0.3.*",
    "antd": "3.6.*",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.*",
    "babel-jest": "22.4.*",
    "babel-plugin-import": "1.7.*",
    "babel-plugin-inline-dotenv": "1.1.*",
    "express": "4.16.*",
    "htmlescape": "1.1.*",
    "prop-types": "15.6.*",
    "less": "3.0.*",
    "next": "6.0.*",
    "react": "16.4.*",
    "react-dom": "16.4.*"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
