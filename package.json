    "babel-plugin-transform-decorators-legacy": "1.3.*",
    "babel-preset-env": "1.7.*",
    "enzyme": "2.9.*",
    "eslint": "4.17.*",
    "eslint-config-airbnb": "16.1.*",
    "eslint-plugin-babel": "4.1.*",
    "eslint-plugin-import": "2.8.*",
    "eslint-plugin-jsx-a11y": "6.0.*",
    "eslint-plugin-react": "7.6.*",
    "jest": "22.4.*",
    "nodemon": "1.17.*",
